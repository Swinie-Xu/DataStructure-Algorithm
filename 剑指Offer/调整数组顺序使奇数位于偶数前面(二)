class Solution {
public:
    /**
     * 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
     *
     * 
     * @param array int整型vector 
     * @return int整型vector
     */
    vector<int> reOrderArrayTwo(vector<int>& array) {
        // write code here
        vector<int> odd;  // 两个辅助数组
        vector<int> even;
        for(const int k:array)
        {
            if(k%2 == 0) even.push_back(k);
            else odd.push_back(k);
        }
        odd.insert(odd.end(),even.begin(),even.end());  // 用了vector的拼接
        return odd;
    }
};
